{
  "name": "@dlr-eoc/core-ui",
  "version": "11.0.0",
  "main": "src/public-api",
  "license": "Apache-2.0",
  "author": "Team UKIS",
  "description": "This project includes schematics to add the base UKIS-Layout to an angular application. The Layout is based on Clarity so [add this first](https://clarity.design/get-started/developing/angular)!",
  "keywords": [
    "schematics",
    "@dlr-eoc",
    "core-ui"
  ],
  "schematics": "./schematics/collection.json",
  "scripts": {
    "preschematics:test": "cd ../../ && ng build core-ui",
    "schematics:test": "npx ../../node_modules/.bin/jasmine ../../dist/core-ui/schematics/**/*_spec.js"
  },
  "ng-add": {
    "save": "dependencies"
  },
  "ng-update": {
    "migrations": "./schematics/migrations/migration.json"
  },
  "dependencies": {
    "@angular-devkit/core": "^14.2.11",
    "@angular-devkit/schematics": "^14.2.11",
    "@schematics/angular": "^14.2.11",
    "parse5-html-rewriting-stream": "^6.0.1",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@angular/common": "^14.2.11",
    "@angular/core": "^14.2.11",
    "@angular/router": "^14.2.11",
    "@clr/angular": "^13.17.0",
    "@clr/icons": "^13.0.2",
    "rxjs": "^6.6.7",
    "jsonc-parser": "^3.1.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "@types/cpx": "^1.5.2",
    "typescript": "^4.8.2",
    "jasmine": "^3.99.0",
    "zone.js": "^0.11.7",
    "@angular/platform-browser-dynamic": "^14.2.11",
    "@angular-devkit/architect": "^0.1402.11"
  }
}
