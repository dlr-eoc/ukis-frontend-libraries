  "bugs": {
    "url": "https://github.com/dlr-eoc/ukis-frontend-libraries/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlr-eoc/ukis-frontend-libraries"
  },
  "keywords": [
    "angular",
    "Clarity Design",
    "openlayers",
    "mapping",
    "OGC"
  ],
  "scripts": {
    "start": "ng serve demo-maps --source-map",
    "build": "node scripts/library/index.js --build",
    "test": "node scripts/library/index.js --test",
    "schematics:test": "jasmine dist/core-ui/schematics/**/*_spec.js",
    "preschematics:test": "ng build --configuration=production --watch=false core-ui",
    "setVersionsInBuild": "node scripts/library/index.js --set",
    "build:scripts": "npx tsc -p scripts/library/tsconfig.json",
    "lint": "ng lint",
    "compodoc": "compodoc -p tsconfig.doc.json -a projects/core-ui/schematics/ng-add/files/src/assets  -n \"UKIS Frontend libraries\"",
    "compodoc-serve": "compodoc -r 8081 -s",
    "postinstall": "cd scripts/library && npm i && cd ../../ && npm run build:scripts"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^12.2.5",
