{
  "name": "ukis-frontend-libraries",
  "version": "7.3.0-next.2",
  "license": "Apache-2.0",
  "author": "Team UKIS",
  "bugs": {
    "url": "https://github.com/dlr-eoc/ukis-frontend-libraries/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlr-eoc/ukis-frontend-libraries"
  },
  "keywords": [
    "angular",
    "Clarity Design",
    "openlayers",
    "mapping",
    "OGC"
  ],
  "scripts": {
    "start": "ng serve demo-maps --sourceMap",
    "build": "node scripts/library/index.js --build",
    "test": "node scripts/library/index.js --test",
    "schematics:test": "jasmine dist/core-ui/schematics/**/*_spec.js",
    "preschematics:test": "ng build core-ui",
    "setVersionsInBuild": "node scripts/library/index.js --set",
    "build:scripts": "npx tsc -p scripts/library/tsconfig.json",
    "lint": "ng lint",
    "compodoc": "compodoc -p tsconfig.doc.json -a projects/core-ui/schematics/ng-add/files/src/assets  -n \"UKIS Frontend libraries\"",
    "compodoc-serve": "compodoc -r 8081 -s",
    "postinstall": "ngcc && cd scripts/library && npm i && cd ../../ && npm run build:scripts"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^10.0.8",
    "@angular-devkit/schematics": "^10.0.8",
    "@angular/animations": "~10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "@clr/angular": "^4.0.0",
    "@clr/core": "4.0.0",
    "@clr/icons": "4.0.0",
    "@clr/ui": "4.0.0",
    "@michaellangbein/jsonix": "3.0.1-SNAPSHOT-3",
    "@schematics/angular": "^10.0.8",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "delaunator": "^4.0.1",
    "md5": "^2.2.1",
    "ogc-schemas": "^2.6.1",
    "ol": "^6.4.3",
    "opensearch-browser": "^2.0.0-alpha.5",
    "parse5-html-rewriting-stream": "^5.1.1",
    "proj4": "^2.6.2",
    "rxjs": "~6.6.0",
    "three": "^0.125.0",
    "tslib": "^2.0.1",
    "w3c-schemas": "^1.4.0",
    "xmlserializer": "^0.6.1",
    "zone.js": "~0.10.3"
  },
  "dependenciesFromProjects": [
    "md5",
    "ol",
    "@types/geojson",
    "@angular/cdk",
    "w3c-schemas",
    "ogc-schemas",
    "@michaellangbein/jsonix",
    "core-js -@clr",
    "web-animations-js -@clr",
    "classlist.js -@clr",
    "@angular-devkit/core -schematics",
    "@angular-devkit/schematics -schematics",
    "@schematics/angular -schematics",
    "cpx -schematics",
    "parse5-html-rewriting-stream -schematics",
    "opensearch-browser - services-ogc"
  ],
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/language-service": "~10.0.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/geojson": "^7946.0.4",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.11.8",
    "@types/three": "^0.103.2",
