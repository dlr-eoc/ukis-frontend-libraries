    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "minimist": "^1.2.0",
    "ng-packagr": "^10.1.0",
    "node-sass": "^4.13.0",
    "protractor": "~7.0.0",
    "terser": "^3.14.1",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  },
  "devDependenciesFromProjects": [
    "@compodoc/compodoc",
    "@types/geojson",
    "depcheck",
    "toposort",
    "replace",
    "terser -owc services",
    "jasmine -schematics",
    "ng-packagr -lib build??",
    "@angular-devkit/build-ng-packagr -lib build??",
    "tslib -lib build??",
    "node-sass -lib build??",
    "minimist -??",
